import java.util.Scanner;

public class Problem654A {
       
    public static Scanner scan=new Scanner(System.in);
    public static void main(String[] args) {
     
       int n;
       n=scan.nextInt();

       while(n-->0){
       
           int data = scan.nextInt();
           System.out.println((int)Math.ceil((double)data/2));   
       }
        scan.close();       
}}
