import java.util.Scanner;

public class Codeforces {
 
    public static Scanner scan = new Scanner(System.in);
    public static void main(String[] args) {
   
        int t=scan.nextInt();
        while(t-->0){
           int a=scan.nextInt();
           int b=scan.nextInt();
           int c=scan.nextInt();
           int d=scan.nextInt();
           System.out.println(b+" "+c+" "+c);
           }
   }}
