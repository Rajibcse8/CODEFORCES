import java.util.Scanner;

public class Codeforces {
 
    public static Scanner scan = new Scanner(System.in);
    public static void main(String[] args) {
        
     int t=scan.nextInt();
     while(t-->0){ 
     int n=scan.nextInt();
     int sum=0;
     int even=0,odd=0;
     for(int i=0;i<n;i++){
         int data=scan.nextInt();
         sum+=data;
        if(data%2==0)even++;
        else odd++;
     }
    
     System.out.println(sum%2==1? "YES":  odd>0&&even>0?"YES":"NO");  
     }
    }}

