import java.util.Scanner;

public class Codeforces {
    
    public static Scanner scan = new Scanner(System.in);
    public static void main(String[] args) {
     
        int n=scan.nextInt();
        int k=scan.nextInt();
     
        for(int i=1;i>0;i++){
            if((n*i)%10==k || (n*i)%10==0){System.out.println(i);return;}
            else continue;
       }  
    }}
