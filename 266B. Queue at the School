import java.util.Scanner;

public class MyPractice {

   
    
    public static Scanner scan=new Scanner(System.in);
    public static void main(String[] args) {
 
       int n=scan.nextInt();
       int t=scan.nextInt();
       String s1=scan.next();
        for(int i=0;i<t;i++){
            s1=s1.replaceAll("BG", "GB");
        }
        System.out.println(s1);
    }}
