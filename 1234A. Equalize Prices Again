import java.util.Scanner;


public class Codeforces {
 
    public static Scanner scan = new Scanner(System.in);
    public static void main(String[] args) {
       int t=scan.nextInt();
       
       while(t-->0){ 
       int n=scan.nextInt();
        int a[]=new int [n];
        int sum=0;
        for(int i=0;i<n;i++){
            a[i]=scan.nextInt();
            sum+=a[i];
            }       
         System.out.println(  (int)  Math.ceil((double) sum/(double) n)  );
         }
    }}
