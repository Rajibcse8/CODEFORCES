import java.util.Scanner;

public class MyPractice {

   
    
    public static Scanner scan=new Scanner(System.in);
    public static void main(String[] args) {
 
        int n=scan.nextInt();
        String s=scan.next();
        s=s.toUpperCase();
        
        int a=0,d=0;
        for(int i=0;i<n;i++){
           if(s.charAt(i)=='A')a++;
           else d++; 
        }
        
        System.out.println(a==d?"Friendship":a>d? "Anton":"Danik");
  
    }}
