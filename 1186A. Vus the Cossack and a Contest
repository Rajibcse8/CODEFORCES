import java.util.Scanner;

public class Codeforces {
 
    public static Scanner scan = new Scanner(System.in);
    public static void main(String[] args) {
      
     int a=scan.nextInt();
     int b=scan.nextInt();
     int c=scan.nextInt();
     
     System.out.println(a<=b && a<=c  ? "Yes": "No");
   
    }}
