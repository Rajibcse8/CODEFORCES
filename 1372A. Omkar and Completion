import java.util.Scanner;


public class Codeforces {
    
  
    
    public static Scanner scan = new Scanner(System.in);
    public static void main(String[] args) {
         int t=scan.nextInt();
         while(t-->0){
         int n=scan.nextInt();
         for(int i=0;i<n;i++)System.out.print(1+" ");
             System.out.println("");
         } 
    }}
