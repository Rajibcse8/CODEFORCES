import java.util.Scanner;

public class Codeforces {
 
    public static Scanner scan = new Scanner(System.in);
    public static void main(String[] args) {
        
       int n=scan.nextInt();
      int count=0;
      for(int i=1;i<=n/2;i++){
        int rem=n-i;
        if(rem%i==0)count++;
      }
        System.out.println(count);
    
    }}
