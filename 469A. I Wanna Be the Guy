import java.util.HashSet;
import java.util.Scanner;
import java.util.Set;

public class Codeforces {

    public static Scanner scan = new Scanner(System.in);
    public static void main(String[] args) {
       
        int n,p,q;
        n=scan.nextInt();
        
       Set<Integer>mus=new HashSet<>();
       p=scan.nextInt(); 
      for(int i=0;i<p;i++){int data=scan.nextInt();mus.add(data);}
      q=scan.nextInt(); 
      for(int i=0;i<q;i++){int data=scan.nextInt();mus.add(data);}
      int len=mus.size();
      mus.clear();
      System.out.println(len==n?"I become the guy.": "Oh, my keyboard!");
            
    }}
