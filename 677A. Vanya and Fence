import java.util.Scanner;

public class MyPractice {
       
    public static Scanner scan=new Scanner(System.in);
    public static void main(String[] args) {
 
      int n=scan.nextInt();
      int h=scan.nextInt();
      int ans=0;
      
      for(int i=0;i<n;i++){
        int data=scan.nextInt();
        
        ans+=(data<=h?1:2);
      }
        System.out.println(ans);
}}
