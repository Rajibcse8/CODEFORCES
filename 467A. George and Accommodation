import java.util.Scanner;

public class MyPractice {

   
    
    public static Scanner scan=new Scanner(System.in);
    public static void main(String[] args) {
 
        int N=scan.nextInt();
        int count=0;
        for(int i=0;i<N;i++){
          int m=scan.nextInt();
          int n=scan.nextInt();
         if(m+2<=n)count++;
        }
        System.out.println(count);
    }}
